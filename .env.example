# BUNK3R - Environment Variables Template
# Copy this file to .env and fill in your values
# Note: In Replit, these are managed in the Secrets tab

# Required - PostgreSQL Database URL
DATABASE_URL=postgresql://user:password@host:5432/database

# Required - Telegram Bot Token from @BotFather
BOT_TOKEN=your_telegram_bot_token_here

# Required - Your Telegram User ID (owner access)
OWNER_TELEGRAM_ID=your_telegram_user_id

# Optional - Telegram Channel ID for notifications
CHANNEL_ID=

# Optional - Admin secret key (auto-generated if not set)
ADMIN_TOKEN=

# Optional - Resend API Key for email notifications
RESEND_API_KEY=
